{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.framework.startCommand": "bun dev",
  "frontMatter.website.host": "https://haobin.liang.london",
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/content/blog",
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": "",
      "filePrefix": null,
      "clearEmpty": true,
      "fields": [
        {
          "title": "Author",
          "name": "author",
          "type": "string",
          "single": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string",
          "required": true
        },
        {
          "title": "Publishing date",
          "name": "pubDatetime",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true,
          "required": true
        },
        {
          "title": "Last updated date",
          "name": "modDatetime",
          "type": "datetime",
          "default": "{{now}}",
          "isModifiedDate": true
        },
        {
          "title": "Is Featured?",
          "name": "featured",
          "type": "boolean"
        },
        {
          "title": "Is Draft?",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Layout",
          "name": "layout",
          "type": "string"
        }
      ]
    }
  ]
}